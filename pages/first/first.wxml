<view class="cu-bar bg-white search " >
  <text class="cuIcon-locationfill lg text-orange"> 广州</text>
  <view class="search-form round">
    <text class="cuIcon-search">  </text>
    <input type="text" placeholder="输入关键字搜索" confirm-type="search" bindinput="searchIcon" ></input>
  </view>
  <!-- <button class="cu-btn icon" role="button" aria-disabled="false">
    <text class="cuIcon-search" bindtap="opinion"></text>
  </button> -->
</view>

<view class="padding bg-grey">
    <view class="flex  p-xs margin-bottom-sm mb-sm">
      <view class="basis-xs  padding-sm margin-xs radius">
        <view>
