        </view>
        <view>
          晴
        </view>
        <view>
          1000
        </view>
      </view>
      <view class="basis-xl padding-sm margin-xs radius">
        
      </view>
    </view>
</view>


<view class="cu-card article {{isCard?'no-card':''}} shadow shadow-lg"> 
    <view class="cu-item shadow">
      <view class="cu-list grid col-{{gridCol}} {{gridBorder?'':'no-border'}}">
        <view class="cu-item" wx:for="{{iconList}}" wx:key wx:if="{{index<gridCol*2}}">
          <a catchtap="toSubway_map" data-name="{{item.name}}">
            <view class="cuIcon-{{item.icon}} text-{{item.color}}">
              <view class="cu-tag badge" wx:if="{{item.badge!=0}}">
                <block wx:if="{{item.badge!=1}}">{{item.badge>99?"99+":item.badge}}</block>
              </view>
            </view>
            <text>{{item.name}}</text>
          </a>
      </view>
    </view>
  </view>
</view>





